<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.h2sj.douyin.domain.mapper.MemberMapper">

    <resultMap type="com.h2sj.douyin.domain.entity.Member" id="member">
        <id column="m_id" property="mId" />
        <result column="m_username" property="mUsername" />
        <result column="m_password" property="mPassword" />
        <result column="m_Gender" property="mGender" />
        <result column="m_portrait" property="mPortrait" />
        <result column="m_birthday" property="mBirthday" />
        <result column="m_email" property="mEmail" />
        <result column="m_phone" property="mPhone" />
        <result column="r_id" property="rId" />
        <association column="r_id" property="mRole" select="com.h2sj.douyin.domain.mapper.RoleMapper.selectById"/>
    </resultMap>

</mapper>